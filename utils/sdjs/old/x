mode=str(sys.argv[1])
print("mode:",mode);
if mode == "off":
	m = "off"
elif mode == "energy_saving":
elif mode == "es":
       m = "es"
#elif mode == "heat_pump":
elif mode == "hp":
	m = "hp"
elif mode == "high demand":
	m = "hd"
elif mode == "hd":
	m = "hd"
