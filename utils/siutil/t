mm="ExtCur ExtCurSlv1 ExtCurSlv2 ExtCurSlv3 TotExtCur InvCur InvCurSlv1 InvCurSlv2 InvCurSlv3 TotInvCur"
while true
do
	siutil -g ${mm} > /tmp/t
	cmd="influx -database 'power' -execute \"insert current "
	i=0
	while read n v
	do
		test $i -gt 0 && cmd="${cmd},"
		cmd="${cmd}${n}=${v}"
		((i++))
	done < /tmp/t
	cmd="$cmd\""
#	echo $cmd
	eval $cmd
#	break
	sleep 10
done
